#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/云笔记/notes/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.5_sass@1.79.2_typesc_5dchmudtuhqw7p7n5p22x7ujfu/node_modules/vuepress/bin/node_modules:/mnt/e/云笔记/notes/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.5_sass@1.79.2_typesc_5dchmudtuhqw7p7n5p22x7ujfu/node_modules/vuepress/node_modules:/mnt/e/云笔记/notes/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.5_sass@1.79.2_typesc_5dchmudtuhqw7p7n5p22x7ujfu/node_modules:/mnt/e/云笔记/notes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/云笔记/notes/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.5_sass@1.79.2_typesc_5dchmudtuhqw7p7n5p22x7ujfu/node_modules/vuepress/bin/node_modules:/mnt/e/云笔记/notes/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.5_sass@1.79.2_typesc_5dchmudtuhqw7p7n5p22x7ujfu/node_modules/vuepress/node_modules:/mnt/e/云笔记/notes/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.15_@types+node@22.5.5_sass@1.79.2_typesc_5dchmudtuhqw7p7n5p22x7ujfu/node_modules:/mnt/e/云笔记/notes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress.js" "$@"
fi
